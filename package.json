{
  "name": "the-great-hack-careem",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "npm-run-all clean compile",
    "clean": "rimraf dist",
    "compile": "babel . --only main.js,src,data --out-dir dist",
    "lint": "eslint main.js src",
    "start": "cross-env NODE_ENV=production node dist/main.js",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node main.js",
    "start:prod": "cross-env NODE_ENV=production npm-run-all clean compile start"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "cross-env": "^6.0.3",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "regenerator-runtime": "^0.13.3",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/register": "^7.7.4",
    "babel-minify": "^0.5.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0"
  }
}
